{
  "langrev": "1",
  "warnings": ["no-unused"],
  "dependency-search-paths": ["lib"],
  "dependencies": ["raylib55", "raygui"],
  "authors": ["Tekin Tontu <tekin@xzone.com.au>"],
  "version": "0.1.0",
  "sources": ["src/common/**"],
  "test-sources": ["test/**"],
  "output": "build",
  "macos-min-version": "15.0",
  "macos-sdk-version": "15.0",
  "targets": {
    "fabrik": {
      "type": "executable",
      "sources": ["src/fabrik/*"]
    },
    "springs": {
      "type": "executable",
      "sources": ["src/springs/*"]
    },
    "tetris": {
      "type": "executable",
      "sources": ["src/tetris/*"]
    },
    "tetris_old": {
      "type": "executable",
      "sources": ["src/tetris_old/*"]
    }
  },
  "cpu": "generic",
  "opt": "O0",
  "debug-info": "full"
}
